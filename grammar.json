{
  "lex": {
    "rules": [
      ["$",                               "return 'EOF'"],
      ["--.*",                            "/* ignore comments */"],
      ["\\s+",                            "/* skip whitespace */"],
      ["<[0-9A-Fa-f][0-9A-Fa-f](\\s+[0-9A-Fa-f][0-9A-Fa-f])*>", "return 'BUFFER'"],
      ["(nil|true|false)\\b",             "return 'CONSTANT'"],
      ["while\\b",                        "return 'WHILE'"],
      ["return\\b",                       "return 'RETURN'"],
      ["abort\\b",                        "return 'ABORT'"],
      ["insert\\b",                       "return 'INSERT'"],
      ["remove\\b",                       "return 'REMOVE'"],
      ["keysof\\b",                       "return 'KEYSOF'"],
      ["typeof\\b",                       "return 'TYPEOF'"],
      ["let\\b",                          "return 'LET'"],
      ["if\\b",                           "return 'IF'"],
      ["elif\\b",                         "return 'ELIF'"],
      ["else\\b",                         "return 'ELSE'"],
      ["\\.&[a-zA-Z_][a-zA-Z0-9_]*\\s*=", "return 'ALIAS'"],
      ["\\.&[a-zA-Z_][a-zA-Z0-9_]*",      "return 'READ'"],
      ["\\.[a-zA-Z_][a-zA-Z0-9_]*\\s*=",  "return 'ASET'"],
      ["\\.[a-zA-Z_][a-zA-Z0-9_]*",       "return 'AGET'"],
      ["\\.(?:0|-?[1-9][0-9]*)\\s*=",     "return 'ISET'"],
      ["\\.(?:0|-?[1-9][0-9]*)",          "return 'IGET'"],
      ["\\$[a-zA-Z_][a-zA-Z0-9_]*",       "return 'NATIVE_CODE'"],
      ["[a-zA-Z_][a-zA-Z0-9_]*:",         "return 'KEY';"],
      ["\"((?:\\.|[^\"])*)\":",           "return 'SKEY'"],
      ["'((?:\\.|[^'])*)':",              "return 'SKEY'"],
      [":+[a-zA-Z_][a-zA-Z0-9_]*",        "return 'SYMBOL'"],
      ["@[a-zA-Z_][a-zA-Z0-9_]*",         "return 'FORM'"],
      ["[a-zA-Z_][a-zA-Z0-9_]*",          "return 'IDENT'"],
      ["(?:0|-?[1-9][0-9]*)",             "return 'INTEGER'"],
      ["\"((?:\\.|[^\"])*)\"",            "return 'STRING'"],
      ["'((?:\\.|[^'])*)'",               "return 'STRING'"],
      ["!\\(",                            "return '!('"],
      ["!",                               "return '!'"],
      ["#",                               "return '#'"],
      ["\\+",                             "return '+'"],
      ["-",                               "return '-'"],
      ["\\*",                             "return '*'"],
      ["\\/",                             "return '/'"],
      ["\\^",                             "return '^'"],
      ["%",                               "return '%'"],
      ["\\?",                             "return '?'"],
      [":",                               "return ':'"],
      ["<=",                              "return '<='"],
      ["<",                               "return '<'"],
      [">=",                              "return '>='"],
      [">",                               "return '>'"],
      ["~=",                              "return '~='"],
      ["~",                               "return '~'"],
      ["==",                              "return '=='"],
      ["=",                               "return '='"],
      ["\\.",                             "return '.'"],
      ["&&",                              "return '&&'"],
      ["\\|\\|",                          "return '||'"],
      ["\\^\\^",                          "return '^^'"],
      ["\\|",                             "return '|'"],
      ["\\&\\[",                          "return '&['"],
      ["\\.&",                            "return '.&'"],
      [":\\[",                            "return ':['"],
      ["\\[",                             "return '['"],
      ["\\]",                             "return ']'"],
      ["\\{",                             "return '{'"],
      ["\\}",                             "return '}'"],
      ["\\(",                             "return '('"],
      ["\\)",                             "return ')'"]
    ]
  },

  "operators": [
    ["right", "=", "ISET", "ASET", "ALIAS"],
    ["right", "?", ":"],
    ["left", "||", "^^"],
    ["left", "&&"],
    ["left", "<", "<=", ">", ">=", "==", "~="],
    ["left", "+", "-", "%"],
    ["left", "*", "/", "^"],
    ["left", "~", "#", "KEYSOF", "TYPEOF"],
    ["left", "!", "!("],
    ["left", ".", "[", "]", "IGET", "AGET", "READ"]
  ],

  "bnf": {
    "root": [
      ["code EOF", "return yy.List([yy.forms.fn, null, yy.List([])].concat($1))"]
    ],
    "code": [
      ["", "$$ = []"],
      ["code expr", "$$ = $1.concat([$2])"],
      ["code statement", "$$ = $1.concat([$2])"]
    ],
    "block": [
      ["{ code }", "$$ = yy.List([yy.forms.block].concat($2))"]
    ],
    "key": [
      ["KEY", "$$ = $1.substr(0, $1.length - 1)"],
      ["SKEY", "$$ = eval($1.substr(0, $1.length - 1))"]
    ],
    "item": [
      ["key expr", "$$ = yy.P($1, $2)"],
      ["expr", "$$ = $1"]
    ],
    "list1": [
      ["item", "$$ = [$1]"],
      ["list1 item", "$$ = $1.concat([$2])"]
    ],
    "list": [
      ["", "$$ = []"],
      ["list1", "$$ = $1"]
    ],
    "params": [
      ["", "$$ = []"],
      ["params IDENT", "$$ = $1.concat([$2])"]
    ],
    "elifs": [
      ["", "$$ = []"],
      ["elifs ELIF expr block", "$$ = $1.concat([$3, $4])"]
    ],
    "expr": [
      ["STRING", "$$ = eval($1);"],
      ["IDENT", "$$ = yy.Symbol($1)"],
      ["INTEGER", "$$ = parseInt($1, 10);"],
      ["CONSTANT", "$$ = $1 === 'true' ? true : $1 === 'false' ? false : null;"],
      ["BUFFER", "$$ = yy.Buffer($1.substr(1, $1.length - 2).split(/\\s+/).map(function (b) { return parseInt(b, 16);}))"],
      ["[ list ]", "$$ = yy.List($2)"],
      ["FORM", "$$ = yy.forms[$1.substr(1)];"],
      ["SYMBOL", "$$ = yy.Symbol($1);"],
      ["NATIVE_CODE", "$$ = yy.NativeCode($1.substr(1));"],
      ["expr !( list )", "$$ = yy.List([yy.forms.call, $1].concat($3))"],
      ["expr !", "$$ = yy.List([yy.forms.call, $1])"],
      ["{ || code }", "$$ = yy.List([yy.forms.def, yy.List([])].concat($3))"],
      ["{ | params | code }", "$$ = yy.List([yy.forms.def, yy.List($3)].concat($5))"],
      ["block", "$$ = $1"],
      ["( expr )", "$$ = $2"],
      ["KEYSOF expr", "$$ = yy.List([yy.forms.keysof, $2])"],
      ["TYPEOF expr", "$$ = yy.List([yy.forms.typeof, $2])"],
      ["expr = expr", "$$ = yy.List([yy.forms.assign, $1, $3])"],
      ["expr + expr", "$$ = yy.List([yy.forms.add, $1, $3])"],
      ["expr - expr", "$$ = yy.List([yy.forms.sub, $1, $3])"],
      ["expr * expr", "$$ = yy.List([yy.forms.mul, $1, $3])"],
      ["expr / expr", "$$ = yy.List([yy.forms.div, $1, $3])"],
      ["expr ^ expr", "$$ = yy.List([yy.forms.pow, $1, $3])"],
      ["expr % expr", "$$ = yy.List([yy.forms.mod, $1, $3])"],
      ["expr < expr", "$$ = yy.List([yy.forms.lt, $1, $3])"],
      ["expr <= expr", "$$ = yy.List([yy.forms.lte, $1, $3])"],
      ["expr > expr", "$$ = yy.List([yy.forms.gt, $1, $3])"],
      ["expr >= expr", "$$ = yy.List([yy.forms.gte, $1, $3])"],
      ["expr == expr", "$$ = yy.List([yy.forms.eq,  $1, $3])"],
      ["expr ~= expr", "$$ = yy.List([yy.forms.neq, $1, $3])"],
      ["expr READ", "$$ = yy.List([yy.forms.read, $1, $2.substr(1)])"],
      ["expr ALIAS expr", "$$ = yy.List([yy.forms.alias, $1, $2.substr(1, $2.match(/[\\s=]/).index - 1), $3])"],
      ["expr AGET", "$$ = yy.List([yy.forms.aget, $1, $2.substr(1)])"],
      ["expr ASET expr", "$$ = yy.List([yy.forms.aset, $1, $2.substr(1, $2.match(/[\\s=]/).index - 1), $3])"],
      ["expr IGET", "$$ = yy.List([yy.forms.get, $1, parseInt($2.substr(1), 10)])"],
      ["expr ISET expr", "$$ = yy.List([yy.forms.set, $1, parseInt($2.substr(1, $2.match(/[\\s=]/).index - 1), 10), $3])"],
      ["~ expr", "$$ = yy.List([yy.forms.not, $2])"],
      ["expr ? expr : expr", "$$ = yy.List([yy.forms.cond, $1, $3, $5])"],
      ["# expr", "$$ = yy.List([yy.forms.len, $2])"],
      ["expr && expr", "$$ = yy.List([yy.forms.and, $1, $3])"],
      ["expr || expr", "$$ = yy.List([yy.forms.or, $1, $3])"],
      ["expr ^^ expr", "$$ = yy.List([yy.forms.xor, $1, $3])"],
      ["LET IDENT = expr", "$$ = yy.List([yy.forms.let, $2, $4]);"],
      ["WHILE expr block", "$$ = yy.List([yy.forms.while, $2, $3])"],
      ["IF expr block elifs", "$$ = yy.List([yy.forms.if, $2, $3].concat($4))"],
      ["IF expr block elifs ELSE block", "$$ = yy.List([yy.forms.if, $2, $3].concat($4).concat([$6]))"]
    ],
    "statement": [
      ["RETURN expr", "$$ = yy.List([yy.forms.return, $2])"],
      ["ABORT expr", "$$ = yy.List([yy.forms.abort, $2])"]
    ]
  }
}
